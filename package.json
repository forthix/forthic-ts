{
  "name": "@forthix/forthic",
  "version": "0.3.0",
  "description": "Stack-based, concatenative language for composable transformations - TypeScript/JavaScript runtime",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js",
      "types": "./dist/cjs/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "forthic": "ts-node scripts/forthic.ts",
    "repl": "ts-node scripts/forthic.ts --repl",
    "pretty": "prettier --print-width 120 --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "prepublishOnly": "npm run build && npm test",
    "test": "jest",
    "test:watch": "jest --watch",
    "docs:build": "npx ts-node scripts/generate-docs.ts"
  },
  "keywords": [
    "forthic",
    "forth",
    "stack-based",
    "concatenative",
    "functional",
    "interpreter",
    "language",
    "dsl",
    "category-theory",
    "composable"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forthix/forthic-ts.git"
  },
  "author": "Rino Jose",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/forthix/forthic-ts/issues"
  },
  "homepage": "https://github.com/forthix/forthic-ts#readme",
  "dependencies": {
    "temporal-polyfill": "^0.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@eslint/js": "^9.15.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.45.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
